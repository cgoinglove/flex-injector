{
  "name": "flex-injector",
  "private": false,
  "version": "0.0.1",
  "description": "A simple dependency injection library for TypeScript. Uses class decorators to inject dependencies, detects circular dependencies, and manages dependencies.",
  "author": "Cgoing",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cgoinglove/flex-injector"
  },
  "keywords": [
    "typescript",
    "dependency-injection",
    "di",
    "injection",
    "injector",
    "class-decorator",
    "decorators",
    "reflect-metadata",
    "ioc",
    "dependency-management"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "eslint src/ --max-warnings 0",
    "test:watch": "vitest",
    "test": "vitest run",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types "
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "reflect-metadata": "^0.2.2",
    "@shared/eslint-config": "workspace:*",
    "@shared/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "@types/node": "^20.11.24",
    "unplugin-swc": "^1.4.5",
    "typescript": "^5.3.3",
    "eslint": "^8.57.0",
    "vitest": "^1.5.0"
  }
}
